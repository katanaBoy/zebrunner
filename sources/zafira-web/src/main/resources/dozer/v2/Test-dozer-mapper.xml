<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://dozer.sourceforge.net http://dozer.sourceforge.net/schema/beanmapping.xsd">

    <mapping type="bi-directional" wildcard="false">
        <class-a>com.qaprosoft.zafira.web.v2.dto.TestDTO</class-a>
        <class-b>com.qaprosoft.zafira.models.db.Test</class-b>
        <field>
            <a>id</a>
            <b>id</b>
        </field>
        <field>
            <a>name</a>
            <b>name</b>
        </field>
        <field>
            <a>testRunId</a>
            <b>testRunId</b>
        </field>
        <field>
            <a>status</a>
            <b>status</b>
        </field>
        <field>
            <a>message</a>
            <b>message</b>
        </field>
    </mapping>

    <mapping type="bi-directional" wildcard="false">
        <class-a>com.qaprosoft.zafira.web.v2.dto.TestResultDTO</class-a>
        <class-b>com.qaprosoft.zafira.models.db.Test</class-b>
        <field>
            <a>status</a>
            <b>status</b>
        </field>
        <field>
            <a>message</a>
            <b>message</b>
        </field>
        <field>
            <a>finishTime</a>
            <b>finishTime</b>
        </field>
    </mapping>
</mappings>
